{
  "name": "react-onsenui",
  "version": "0.1.0",
  "description": "React extension for Onsen UI",
  "main": "dist/react-onsenui.js",
  "scripts": {
    "build": "npm run lint && rollup -c",
    "prepublish": "npm run build",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --ext .js,.jsx src --fix",
    "watch": "nodemon -x \"npm run build\" --watch src -e jsx",
    "dev": "npm run watch & watchify -vd -t [ babelify --presets es2015,react --ignore OnsenUI/build/js/onsenui.js ] -t [ aliasify --aliases [ --react-onsenui=./dist/react-onsenui.js ] ] -e demo/index.js -o demo/bundle.js & http-server . -c 1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argelius/react-onsenui.git"
  },
  "keywords": [
    "react",
    "react-component",
    "cordova"
  ],
  "author": "Onsen UI Team <team@monaca.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/argelius/react-onsenui/issues"
  },
  "homepage": "https://github.com/argelius/react-onsenui#readme",
  "devDependencies": {
    "aliasify": "^1.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "eslint": "^2.5.3",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "nodemon": "^1.9.1",
    "onsenui": "2.0.0-beta.9",
    "react": "^0.14.8",
    "react-docgen": "2.8.2",
    "react-dom": "^0.14.8",
    "rollup": "^0.25.7",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-replace": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
